{
  "name": "lkdevelopment/hetzner-cloud-php-sdk",
  "description": "A inofficial PHP SDK for the Hetzner Cloud API.",
  "type": "library",
  "keywords": [
    "hetzner",
    "hcloud",
    "hetzner cloud",
    "cloud php library"
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "tightenco/collect": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.14",
    "brianium/paratest": "1.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas KÃ¤mmerling",
      "email": "kontakt@lukas-kaemmerling.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "LKDev\\HetznerCloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "docs": "php phpDocumentor.phar -d ./src -t ./docs"
  }
}
